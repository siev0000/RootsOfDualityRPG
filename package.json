{
  "name": "rpgjs",
  "version": "5.0.0-alpha.11",
  "scripts": {
    "build": "tsx bin/build.ts",
    "dev": "tsx bin/dev.ts",
    "clean": "tsx bin/clean.ts",
    "release": "bumpp package.json packages/*/package.json --all",
    "rpgjs:version": "tsx bin/version.ts",
    "rpgjs:publish": "pnpm -r publish --publish-branch v5 --no-git-checks --access public --force"
  },
  "author": "Samuel Ronce",
  "license": "MIT",
  "description": "RPGJS is a framework for creating RPG/MMORPG games",
  "dependencies": {
    "bumpp": "^10.1.1",
    "execa": "^9.5.2",
    "pnpm": "^10.12.1",
    "tsx": "^4.19.3",
    "wait-on": "^8.0.3"
  },
  "keywords": [
    "rpg",
    "game",
    "engine",
    "javascript",
    "typescript"
  ],
  "private": true,
  "type": "module",
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@types/node": "^24.0.1"
  }
}
